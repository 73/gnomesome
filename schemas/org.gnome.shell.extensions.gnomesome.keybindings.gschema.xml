<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
    <schema id="org.gnome.shell.extensions.gnomesome.keybindings" path="/org/gnome/shell/extensions/gnomesome.keybindings/">
        <key type="as" name="next-window">
            <default>["&lt;Mod4&gt;j"]</default>
            <summary>Select the next window.</summary>
            <description>Select the next window on the current screen and workspace.</description>
        </key>
        <key type="as" name="previous-window">
            <default>["&lt;Mod4&gt;k"]</default>
            <summary>Select the previous window.</summary>
            <description>Select the previous window on the current screen and workspace.</description>
        </key>
        <key type="as" name="next-monitor">
            <default>["&lt;Mod4&gt;&lt;Ctrl&gt;j"]</default>
            <summary>Select the next monitor.</summary>
            <description>Select the topmost window on the next screen and current workspace.</description>
        </key>
        <key type="as" name="previous-monitor">
            <default>["&lt;Mod4&gt;&lt;Ctrl&gt;k"]</default>
            <summary>Select the previous monitor.</summary>
            <description>Select the topmost window on the previous screen and current workspace.</description>
        </key>
        <key type="as" name="move-to-next-monitor">
            <default>["&lt;Mod4&gt;o"]</default>
            <summary>Move the current window to the next monitor</summary>
            <description>Move the current focused window to the next monitor on the current workspace.</description>
        </key>
        <key type="as" name="set-workspace-1">
            <default>["&lt;Mod4&gt;1"]</default>
            <summary>Select the workspace 1.</summary>
            <description>Select the workspace with id 1.</description>
        </key>
        <key type="as" name="set-workspace-2">
            <default>["&lt;Mod4&gt;2"]</default>
            <summary>Select the workspace 2.</summary>
            <description>Select the workspace with id 2.</description>
        </key>
        <key type="as" name="set-workspace-3">
            <default>["&lt;Mod4&gt;3"]</default>
            <summary>Select the workspace 3.</summary>
            <description>Select the workspace with id 3.</description>
        </key>
        <key type="as" name="set-workspace-4">
            <default>["&lt;Mod4&gt;4"]</default>
            <summary>Select the workspace 4.</summary>
            <description>Select the workspace with id 4.</description>
        </key>
        <key type="as" name="set-workspace-5">
            <default>["&lt;Mod4&gt;5"]</default>
            <summary>Select the workspace 5.</summary>
            <description>Select the workspace with id 5.</description>
        </key>
        <key type="as" name="launch-terminal">
            <default>["&lt;Mod4&gt;return"]</default>
            <summary>Lanch an terminal.</summary>
            <description>Launches a gnome terminal.</description>
        </key>
    </schema>
</schemalist>      
